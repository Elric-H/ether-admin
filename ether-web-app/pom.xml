        </dependency>

        <!-- 代码生成插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-gen</artifactId>
        </dependency>

        <!-- 移动端管理插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-mobile</artifactId>
        </dependency>

        <!-- 系统功能插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-sys</artifactId>
        </dependency>

        <!-- 动态租户插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-ten</artifactId>
        </dependency>

        <!-- 报表设计插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-urp</artifactId>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <version>2.5.12</version>
                <configuration>
                    <includeSystemScope>true</includeSystemScope>
                </configuration>
                <executions>
                    <execution>
                        <goals>
                            <goal>repackage</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>
