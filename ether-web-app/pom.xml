        <!-- 登录鉴权插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-auth</artifactId>
        </dependency>

        <!-- 业务功能插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-biz</artifactId>
        </dependency>

        <!-- C端功能插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-client</artifactId>
        </dependency>

        <!-- 多数据源插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-dbs</artifactId>
        </dependency>

        <!-- 开发工具插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-dev</artifactId>
        </dependency>

        <!-- 功能案例插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-exm</artifactId>
        </dependency>

        <!-- 工作流程插件 -->
        <dependency>
            <groupId>com.xuan</groupId>
            <artifactId>ether-plugin-flw</artifactId>
